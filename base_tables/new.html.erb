<h1> Create a new base table </h1>

<%= form_with(model: @base, local: true) do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name %>

    <%=f.label :conF_string %>
    <%=f.collection_select(:conF_string, Confirmer.all, :position, :name, {:include_blank => "Please Select", :include_hidden => false}, {:multiple => true}) %>
    <%=f.submit %>
<% end %>
